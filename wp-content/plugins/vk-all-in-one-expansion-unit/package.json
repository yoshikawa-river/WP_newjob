{
  "name": "vk-all-in-one-expansion-unit",
  "version": "9.11.3",
  "description": " This plug-in is an integrated plug-in with a variety of features that make it powerful your web site. Many features can be stopped individually. Example Facebook Page Plugin,Social Bookmarks,Print OG Tags,Print Twitter Card Tags,Print Google Analytics tag,New post widget,Insert Related Posts and more!",
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@wordpress/babel-plugin-makepot": "^4.2.0",
    "@wordpress/env": "^4.1.3",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "child_process": "^1.0.2",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-merge-media-queries": "^0.2.1",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-sass": "^5.0.0",
    "gulp-uglify": "^3.0.2",
    "npm-check-updates": "^12.0.5",
    "po2json": "^1.0.0-beta-3",
    "run-sequence": "^2.2.1",
    "sass": "^1.45.1"
  },
  "scripts": {
    "start": "npx wp-env start",
    "stop": "npx wp-env stop",
    "dist": "composer install --no-dev && bash bin/dist",
    "gulp": "gulp",
    "watch": "gulp watch",
    "build": "npx gulp text-domain && npx gulp build",
    "translate": "po2json ./languages/ja_JP.po ./languages/veu-block-ja-veu-block.json -f jed",
    "phpunit": "npx wp-env run phpunit 'phpunit -c /var/www/html/wp-content/plugins/vk-all-in-one-expansion-unit/phpunit.xml.dist --verbose'"
  },
  "repository": {
